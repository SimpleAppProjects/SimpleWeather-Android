<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BottomStepperNavigationBar">
        <attr name="nav_backgroundColor" format="color|reference" />
        <attr name="nav_foregroundColor" format="color|reference" />

        <attr name="nav_startButtonText" format="string|reference" />
        <attr name="nav_backButtonText" format="string|reference" />
        <attr name="nav_nextButtonText" format="string|reference" />
        <attr name="nav_completeButtonText" format="string|reference" />

        <attr name="nav_backButtonIcon" format="reference" />
        <attr name="nav_nextButtonIcon" format="reference" />
        <attr name="nav_completeButtonIcon" format="reference" />
    </declare-styleable>
    <declare-styleable name="FlowLayout">
        <!-- Horizontal spacing between two items being laid out. -->
        <attr format="dimension" name="itemSpacing" />
        <!-- Vertical Spacing between two lines of items being laid out. -->
        <attr format="dimension" name="lineSpacing" />
    </declare-styleable>
    <declare-styleable name="FlowLayout_Layout">
        <attr name="layout_itemMinWidth" format="dimension" />
    </declare-styleable>
    <declare-styleable name="AspectRatioFrameLayout">
        <attr name="aspectRatio" format="float" />
        <attr name="maxHeight" format="dimension" />
        <attr name="maxWidth" format="dimension" />
        <attr name="enableAspectRatio" format="boolean" />
    </declare-styleable>
</resources>